{
	"name": "vscode-upy",
	"displayName": "vscode-upy",
	"description": "MicroPython workflow for vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.loadRam",
		"onCommand:extension.dumpMem",
		"onCommand:extension.makeImage"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.loadRam",
				"title": "upy: load ram"
			},
			{
				"command": "extension.dumpMem",
				"title": "upy: dump memory"
			},
			{
				"command": "extension.makeImage",
				"title": "upy: make image"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.9",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/rimraf": "^2.0.3",
		"@types/vscode": "^1.42.0",
		"chai": "^4.2.0",
		"fsify": "^3.0.0",
		"glob": "^7.1.5",
		"mocha": "^7.0.1",
		"rimraf": "^3.0.2",
		"ts-mockito": "^2.5.0",
		"tslint": "^5.20.1",
		"typescript": "^3.6.4",
		"vscode-extension-tester": "^2.4.0",
		"vscode-test": "^1.2.2"
	}
}
